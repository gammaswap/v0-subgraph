{
  "name": "position-manager",
  "version": "1.0.0",
  "license": "GPL-3.0",
  "scripts": {
    "codegen": "yarn prepare && graph codegen",
    "build": "graph build",
    "create": "graph create PositionManager --node https://api.thegraph.com/deploy/",
    "create-local": "graph create PositionManager --node http://127.0.0.1:8020",
    "prepare": "mustache config/localhost.json subgraph.template.yaml > subgraph.yaml",
    "deploy": "graph deploy PositionManager --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy PositionManager --ipfs http://127.0.0.1:5001 --node http://127.0.0.1:8020",
    "remove-local": "graph remove --node http://127.0.0.1:8020/ PositionManager",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.33.0",
    "@graphprotocol/graph-ts": "0.27.0"
  },
  "devDependencies": {
    "matchstick-as": "0.5.0",
    "mustache": "^4.2.0"
  }
}
